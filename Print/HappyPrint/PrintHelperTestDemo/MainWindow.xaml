<Window x:Class="PrintHelperTestDemo.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525" x:Name="window">
    <Grid>
        <Button x:Name="Btn" Width="100" Height="30" Content="DataTable" Click="ButtonBase_OnClick"></Button>
        <Button Width="100" Height="30" Margin="0,100,0,0" Content="Image" Click="ButtonBase2_OnClick"></Button>
        <Button Width="100" Height="30" Margin="0,200,0,0" Content="Control" Click="ButtonBase3_OnClick"></Button>
        <Image x:Name="image" Width="100" Height="100" Margin="0,-200,0,0"></Image>
        <Rectangle Width="100" Height="100" Margin="-400,-200,0,0" Fill="Green"></Rectangle>
    </Grid>
</Window>
